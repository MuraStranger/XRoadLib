{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "nowarn": ["CS1591"],
    "outputName": "dotnet-xroad-gen",
    "warningsAsErrors": true
  },
  "configurations": {
    "Release": {
      "buildOptions": {
        "keyFile": "../XRoadLib.snk"
      }
    }
  },
  "authors": ["Janno PÃµldma"],
  "description": "Command line tool to generate C# types from service description.",
  "packOptions": {
    "projectUrl": "https://github.com/janno-p/XRoadLib",
    "licenseUrl": "https://github.com/janno-p/XRoadLib/blob/master/LICENSE.txt",
    "tags": ["xroad", "x-road", "xtee", "x-tee", "code", "type", "generator", "wsdl", "service", "description"],
    "summary": "XRoadLib code generator"
  },
  "dependencies": {
    "Microsoft.CodeAnalysis": "1.3.2",
    "Microsoft.Extensions.CommandLineUtils": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
    "XRoadLib": {
      "target": "project",
      "version": "1.0.0-*"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": "portable-net45+win8+wp8+wpa81"
    }
  }
}
